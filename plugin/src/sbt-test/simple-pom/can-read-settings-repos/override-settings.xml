<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <activeProfiles>
    <activeProfile>an-active-profile</activeProfile>
  </activeProfiles>
    <profiles>
        <profile>
            <id>an-active-profile</id>
            <repositories>
                <repository>
                    <id>sbt-bintray</id>
                    <url>https://dl.bintray.com/sbt/sbt-plugin-releases</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>                    
                </repository>
            </repositories>
        </profile>
        <profile>
            <id>an-inactive-profile</id>
            <repositories>
                <repository>
                    <id>one-sad-repo</id>
                    <url>https://maven.java.net/content/groups/public</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
            </repositories>
        </profile>
    </profiles>
</settings>
